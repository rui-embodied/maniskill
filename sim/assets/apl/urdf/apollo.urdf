  <link name="camera_pos">
  </link>
  <joint name="camera_arm" type="fixed">
    <origin rpy="0 0 -1.57079632679" xyz="0 5 3"/>
    <parent link="base_link"/>
    <child link="camera_pos"/>
  </joint>
</robot>